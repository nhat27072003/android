            app:layout_constraintEnd_toStartOf="@+id/guideline4"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintTop_toTopOf="@+id/btn6"
            app:layout_constraintWidth_percent="0.23" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn6"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#EDEDEE"
            android:text="6"
            android:textSize="25sp"
            app:cornerRadius="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/btnSubtract"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@+id/btn3"
            app:layout_constraintStart_toEndOf="@+id/btn5"
            app:layout_constraintStart_toStartOf="@+id/btn3"
            app:layout_constraintTop_toTopOf="@+id/btnSubtract"
            app:layout_constraintWidth_percent="0.23" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSubtract"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            android:background="#e0e0e0"
            android:text="-"
            android:textSize="25sp"
            app:cornerRadius="0dp"
            app:layout_constraintBottom_toTopOf="@+id/btnAdd"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@+id/btnAdd"
            app:layout_constraintStart_toEndOf="@+id/btn6"
            app:layout_constraintStart_toStartOf="@+id/btnAdd"
            app:layout_constraintTop_toBottomOf="@+id/btnMultiple"
            app:layout_constraintWidth_percent="0.23" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.25124377" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.7542579" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#EDEDEE"
            android:text="1"
            android:textSize="25sp"
            app:cornerRadius="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/btn2"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@+id/btnMore"
            app:layout_constraintStart_toStartOf="@+id/btnMore"
            app:layout_constraintTop_toTopOf="@+id/btn2"
            app:layout_constraintWidth_percent="0.23" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#EDEDEE"
            android:text="2"
            android:textSize="25sp"
            app:cornerRadius="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/btn3"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@+id/btn0"
            app:layout_constraintStart_toEndOf="@+id/btn1"
            app:layout_constraintStart_toStartOf="@+id/btn0"
            app:layout_constraintTop_toTopOf="@+id/btn3"
            app:layout_constraintWidth_percent="0.23" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#EDEDEE"
            android:text="3"
            android:textSize="25sp"
            app:cornerRadius="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/btnAdd"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@+id/btn."
            app:layout_constraintStart_toEndOf="@+id/btn2"
            app:layout_constraintStart_toStartOf="@+id/btn."
            app:layout_constraintTop_toTopOf="@+id/btnAdd"
            app:layout_constraintWidth_percent="0.23" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnAdd"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            android:background="#e0e0e0"
            android:text="+"
            android:textSize="25sp"
            app:cornerRadius="0dp"
            app:layout_constraintBottom_toTopOf="@+id/btnEqual"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@+id/btnEqual"
            app:layout_constraintStart_toEndOf="@+id/btn3"
            app:layout_constraintStart_toStartOf="@+id/btnEqual"
            app:layout_constraintTop_toBottomOf="@+id/btnSubtract"
            app:layout_constraintWidth_percent="0.23" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnMore"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="2dp"
            android:background="#e0e0e0"
            android:text="+/-"
            android:textSize="25sp"
            app:cornerRadius="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/btn0"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toStartOf="@+id/guideline3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btn0"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintWidth_percent="0.23" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn0"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="2dp"
            android:background="#EDEDEE"
            android:text="0"
            android:textSize="25sp"
            app:cornerRadius="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/btn."
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toStartOf="@+id/guideline4"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintTop_toTopOf="@+id/btn."
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintWidth_percent="0.23" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn."
            android:layout_width="0dp"
            android:layout_height="0dp"

            android:background="#e0e0e0"
            android:text="."
            android:textSize="25sp"
            app:cornerRadius="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/btnEqual"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toStartOf="@+id/guideline5"
            app:layout_constraintStart_toStartOf="@+id/guideline4"
            app:layout_constraintTop_toTopOf="@+id/btnEqual"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintWidth_percent="0.23" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnEqual"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="4dp"
            android:background="#e0e0e0"
            android:text="="
            android:textSize="25sp"
            app:cornerRadius="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/guideline5"
            app:layout_constraintTop_toBottomOf="@+id/btnAdd"
            app:layout_constraintWidth_percent="0.23" />

